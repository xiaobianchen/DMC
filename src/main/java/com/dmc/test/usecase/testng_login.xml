<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="DMCTestSuite" parallel="false" verbose="1" thread-count="15">
<parameter name="webSessionTimeOut" value="60000"/>
<parameter name="browser" value="chrome"/>
<parameter name="localizedBuild" value="true" />
<parameter name="openReportInBrowser" value="firefox" />
<parameter name="url-config" value="" />
  
  <test name="Test Login Page">
	  <groups>
	  	<run>
	  		<include name="login" />
	  	</run>
	  </groups>
	  <classes>
			<class name="com.dmc.test.usecase.LoginTestPlan" />
     </classes>
  </test>
    
</suite>
